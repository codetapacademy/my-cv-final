-
  var header = '#what I can do'
  var whatICanDoList = [
    {
      title: 'Front End',
      skillList: [
        'React',
        'Redux',
        'HTML5',
        'CSS3',
        'Galaxy defender',
        'UltraSuperMan',
        'Flower picker'
      ]
    },
    {
      title: 'BackEnd',
      skillList: [
        'FireBase',
        'Git',
        'CLI',
        'Pupet master',
        'Virtual dreamer',
        'Spit lava angry dude'
      ]
    },
    {
      title: 'Other',
      skillList: [
        'shy and bold',
        'love to create',
        'create to love',
        'the sky is'
      ]
    }
  ];
#what-i-can-do
.what-i-can-do
  .what-i-can-do__header=header
  each whatICanDo in whatICanDoList
    .what-i-can-do__title=whatICanDo.title
    .what-i-can-do__list
      each skill in whatICanDo.skillList
        .what-i-can-do__skill=skill